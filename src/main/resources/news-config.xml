<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="djNewsProvider" class="com.springjiemi.ioc.FXNewsProvider">
        <constructor-arg index="0" ref="djNewsListener"/>
        <constructor-arg index="1" ref="djNewsPersister"/>
    </bean>
    <bean id="djNewsListener" class="com.springjiemi.ioc.DowJonesNewsListener"/>
    <bean id="djNewsPersister" class="com.springjiemi.ioc.DowJonesNewsPersister"/>


</beans>